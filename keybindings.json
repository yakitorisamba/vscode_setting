[
    {
        "key": "ctrl+t",
        "command": "workbench.action.terminal.focus",
        "when": "editorTextFocus" ,
      },
      {
        "key": "ctrl+p",
        "command": "workbench.action.quickOpenNavigatePrevious",
        "when": "inQuickOpen",
      
      },
      {
        "key": "ctrl+n",
        "command": "workbench.action.quickOpenNavigateNext",
        "when": "inQuickOpen",
      },
      {
          "key": "ctrl+p",
          "command": "workbench.action.terminal.toggleTerminal",
          "when": "vim.mode != 'SearchInProgressMode' && vim.mode != 'Insert'"
      },
      {
          "key": "ctrl+h",
          "command": "workbench.action.previousEditor",
        "when": "vim.mode != 'Insert'"
      },
      {
          "key": "ctrl+l",
          "command": "-workbench.action.nextEditor",
        "when": "vim.mode != 'Insert'"
      },
      {
        "key": "ctrl+t",
        "command": "-workbench.action.showAllSymbols"
      },
      {
          "key": "ctrl+p",
          "command": "cursorUp",
          "when": "terminalFocus"
      },
      {
          "key": "ctrl+n",
          "command": "cursorDown",
          "when": "terminalFocus"
      },
      {
          "key": "ctrl+f",
          "command": "cursorRight",
          "when": "terminalFocus"
      },
      {
          "key": "ctrl+b",
          "command": "cursorLeft",
          "when": "terminalFocus"
      },
      {
          "key": "ctrl+a",
          "command": "cursorHome",
          "when": "terminalFocus"
      },
      {
          "key": "ctrl+e",
          "command": "cursorEnd",
          "when": "terminalFocus"
      },
      {
          "key": "ctrl+d",
          "command": "deleteRight",
          "when": "terminalFocus"
      },
      {
          "key": "ctrl+h",
          "command": "deleteLeft",
          "when": "terminalFocus"
      },
      {
          "key": "ctrl+k",
          "command": "deleteAllRight",
          "when": "terminalFocus"
      },
]
